{
  "rust-analyzer.hoverActions.linksInHover": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "css",
    "markdown",
    "json",
    "javascriptreact",
    "typescriptreact",
    "graphql"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "cSpell.language": "en-GB",
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll",
    "*.vim"
  ],
  "diagnostic.errorSign": "✗",
  "diagnostic.warningSign": "∆",
  "diagnostic.infoSign": "!",
  "diagnostic.hintSign": "#"
}
